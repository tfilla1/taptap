{
  "name": "taptap",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-pages": "run-p type-check \"build-pages-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "build-pages-only": "vite build --base '/cards/'",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "@vueuse/core": "^10.5.0",
    "core-js": "^3.29.0",
    "howler": "^2.2.4",
    "paper": "^0.12.17",
    "roboto-fontface": "*",
    "vuetify": "^3.0.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/p5": "^1.7.1",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^18.17.15",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "@types/howler": "^2.2.10",
    "sass": "^1.60.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "~5.1.6",
    "unplugin-fonts": "^1.0.3",
    "vite": "^4.4.9",
    "vitest": "^0.34.4",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.8.11"
  }
}
